 \n        data.colors = colorsList.map((color) => color.colorName)\n        data.images = imagesList\n        console.log(data)\n        CreateProduct(data)\n       \n\n    }\n    const CreateProduct = (data) => {\n        const formData = new FormData();\n        formData.append('name', data.name);\n        formData.append('price', data.price);\n        formData.append('type', data.type);\n        formData.append('quantity', data.quantity);\n        formData.append('colors', data.colors)\n        data.images.forEach(image => {\n            formData.append('images', image)\n        });\n        const headers = {\n            'Content-Type': 'multipart/form-data',\n            Authorization: 'Barear eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJlbWFpbCI6Im1va2h0YXJraGVsbGFkaTEyQGdtYWlsLmNvbSIsInVzZXJJZCI6IjY0MWM5MmRmMjNmNjRmMzEwMGFkZmEzZSIsImlhdCI6MTY4MDE4MjQxOSwiZXhwIjoxNjgwMTg2MDE5fQ.vJTee51RVH31_-RWtPagmYGq5AVv0lWb7p0ysDPmC28'\n        }\n\n        setLoading(true)\n        axios.put(\"http://localhost:3001/admin/product\", formData, { headers })\n            .then(response => {\n                setLoading(false)\n                const message = response.data.message\n                notify(message, \"Success\")\n                reset()\n                setColorsList([])\n                setImagesList([])\n\n            })\n            .catch(error => {\n                setLoading(false)\n               \n                const message = error.response.data.message\n                notify(message, \"Error\")\n            });\n\n\n    }\n\n\n\n    const handleChange = (event) => {\n        setNewColor(event.target.value)\n        setInputValue(event.target.value)\n    }\n    const addColor = () => {\n        if (inputValue !== \"\") {\n            const color = {\n                id: colorsList.length === 0 ? 1 : colorsList[colorsList.length - 1].id + 1,\n                colorName: newColor\n            }\n\n            setColorsList([...colorsList, color])\n        }\n        setInputValue(\"\")\n\n    }\n\n\n\n\n    const deleteColor = function (id) {\n        console.log(id)\n        const newColorsList = colorsList.filter((color) => {\n            if (color.id === id) {\n                return false\n            } else {\n                return true\n            }\n        })\n        setColorsList(newColorsList)\n    }\n    const deleteImage = function (id) {\n        const newImagesList = imagesList.filter((image) => {\n            if (id === imagesList.indexOf(image)) {\n                return false\n            } else {\n                return true\n            }\n        })\n        setImagesList(newImagesList)\n\n\n    }\n\n    const handleImage = (e) => {\n\n        const selectedFiles = e.target.files;\n        setImagesList([...imagesList, ...selectedFiles])\n\n\n    }\n    // to testify notfication function\n    const notify = (message, type) => {\n        if (type === \"Error\") {\n            toast.error(message);\n        } else if (type === \"Success\") {\n            toast.success(message);\n        }\n    }\n\n\n\n\n\n\n    return (\n        <>\n        {loading ? <Loading /> : ( \n        <div className='main-container'>\n          \n               \n\n                 <div className=\"prod-container\">\n                 <div className=\"form-container\">\n                     <form onSubmit={handleSubmit(onSubmit)}>\n                         <h4>Product Name</h4>\n                         <input type=\"text\" placeholder='Addidas Ultra Boost' {...register(\"name\")} />\n                         <p>{errors.name?.message}</p>\n                         <h4>Price of the Product</h4>\n                         <input type=\"number\" step=\"0.01\"  placeholder='the price' {...register(\"price\")}  />\n                         <p>{errors.price?.message}</p>\n                         <h4>Type</h4>\n                         <input type=\"text\" placeholder='Sneaker' {...register(\"type\")} />\n                         <p>{errors.type?.message}</p>\n                         <h4>Stock Quantity</h4>\n                         <input type=\"number\" placeholder='How much we have?' {...register(\"quantity\")}   />\n                         <p>{errors.quantity?.message}</p>\n                         <h4>Colors</h4>\n                         {/* the colors section */}\n                         <div className=\"addColor\">\n                             <input onChange={handleChange} value={inputValue} type=\"text\" />\n                             <div onClick={addColor} id=\"addColor\">Add Colors</div>\n                         </div>\n                         <div className='ColorsList'>\n                             {colorsList.map((color, index) => {\n                                 return <Color key={index} colorName={color.colorName} id={color.id} deleteColor={deleteColor} />\n                             })}\n                         </div>\n                         <h4>Upload The Photos of the Product here</h4>\n \n                         <button type=\"button\" className=\"upload-imgs\" onClick={() => fileInputRef.current.click()} {...register(\"file\")} >upload pictures</button>\n                         <input  multiple accept=\"image/*\" type=\"file\" tabIndex={-1}  className=\"hiddenInput\" ref={fileInputRef}\n                             onChange={handleImage}   />\n                        <p >{errors.file?.message}</p>\n                         <div className='ImagesList'>\n                             {imagesList.map((image, index) => {\n                                 var index = imagesList.indexOf(image)\n                                 return <Image  key={index} imageName={image.name} id={index} deleteImage={deleteImage} />\n                             })}\n                         </div>\n \n \n                         <div className=\"btns\">\n                             <button id=\"u-btn\">Upload Product</button>\n                             <button type=\"button\" id=\"c-btn\" onClick={ () => {\n                                 navigate('/')\n                             }}>Cancel</button>\n                         </div>\n                  \n                     </form>\n                 \n                 </div>\n \n \n \n \n             </div>\n \n           \n          \n           \n        \n\n            <ToastContainer style={{ width: \"400px\" }} />\n\n        </div>\n       \n        )}\n\n    \n    </>\n    )\n\n  \n}","/home/pain/Desktop/project/admin/src/pages/adminDashboard.js",["114","115","116","117","118"],[],"/home/pain/Desktop/project/admin/src/components/Color.js",[],[],"/home/pain/Desktop/project/admin/src/components/Image.js",[],[],"/home/pain/Desktop/project/admin/src/components/loading.js",[],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/index.js",[],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/Admin.js",["119"],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/components/header.js",[],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/components/navbar.js",[],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/pages/createProduct.js",["120","121"],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/components/footer.js",["122","123","124","125"],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/pages/adminDashboard.js",["126","127","128","129"],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/components/Image.js",[],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/components/Color.js",[],[],"/home/pain/Desktop/test/admin-dashboard-ecommerce/src/components/loading.js",[],[],{"ruleId":"130","severity":1,"message":"131","line":7,"column":50,"nodeType":"132","messageId":"133","endLine":7,"endColumn":54},{"ruleId":"134","severity":1,"message":"135","line":9,"column":17,"nodeType":"136","endLine":9,"endColumn":28},{"ruleId":"134","severity":1,"message":"135","line":10,"column":17,"nodeType":"136","endLine":10,"endColumn":28}